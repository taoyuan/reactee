{
  "name": "reactee",
  "version": "0.3.0",
  "description": "A React SideBar component",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --inline --content-base examples/",
    "clean": "rimraf lib",
    "build:commonjs": "babel src/ --out-dir lib/ --ignore __tests__,__mocks__",
    "build:umd": "webpack --devtool source-map --config webpack.build.js",
    "build:umd:min": "cross-env MINIFY=1 webpack --devtool source-map --config webpack.build.js",
    "build": "npm run clean && npm run build:commonjs",
    "bundle": "mkdir -p dist && npm run build:umd && npm run build:umd:min",
    "lint": "eslint src",
    "preversion": "npm test && npm run bundle && git commit -m 'Publish: build distribution'",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taoyuan/reactee.git"
  },
  "author": "Yuan Tao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taoyuan/reactee/issues"
  },
  "files": [
    "dist",
    "lib"
  ],
  "homepage": "https://github.com/taoyuan/reactee",
  "keywords": [
    "react",
    "ui",
    "sidebar",
    "sidenav",
    "popover",
    "popup",
    "react-component"
  ],
  "peerDependencies": {
    "react": "^0.14.7 || ^15.1.0",
    "react-dom": "^0.14.7 || ^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "jest-cli": "^18.1.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "postcss": "^5.2.12",
    "postcss-loader": "^1.3.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-icons": "^2.2.3",
    "react-modal": "^1.6.5",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "classnames": "^2.2",
    "inline-style-prefixer": "^2.0.5",
    "invariant": "^2.2.2",
    "lodash": "^4.17.4",
    "object-assign": "^4.1.1",
    "react-addons-shallow-compare": "^15.4.2",
    "react-avatar": "^2.3.0",
    "react-icon-base": "^2.0.4",
    "react-motion": "^0.4.7",
    "react-overlays": "^0.6.10",
    "recompose": "^0.22.0",
    "warning": "^3.0.0"
  },
  "jest": {
    "automock": false,
    "testPathDirs": [
      "src"
    ],
    "unmockedModulePathPatterns": [
      "node_modules",
      "babel"
    ]
  }
}
