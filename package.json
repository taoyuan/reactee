{
  "name": "reactee-sidebar",
  "version": "0.1.0",
  "description": "A React SideBar component",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --content-base examples/",
    "clean": "rimraf lib",
    "build:commonjs": "babel src/ --out-dir lib/ --ignore __tests__,__mocks__",
    "build:umd": "webpack --devtool source-map --config webpack.build.js",
    "build:umd:min": "cross-env MINIFY=1 webpack --devtool source-map --config webpack.build.js",
    "build": "npm run clean && npm run build:commonjs",
    "bundle": "mkdir -p dist && npm run build:umd && npm run build:umd:min",
    "lint": "eslint src",
    "preversion": "npm test && npm run bundle && git commit -m 'Publish: build distribution'",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taoyuan/reactee-sidebar.git"
  },
  "author": "Yuan Tao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taoyuan/reactee-sidebar/issues"
  },
  "files": [
    "dist",
    "lib"
  ],
  "homepage": "https://github.com/taoyuan/reactee-sidebar",
  "keywords": [
    "react",
    "routee",
    "router",
    "route",
    "react-component"
  ],
  "peerDependencies": {
    "react": "^0.14.7 || ^15.1.0",
    "react-dom": "^0.14.7 || ^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jest-cli": "^12.1.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-icons": "^2.1.0",
    "react-modal": "^1.3.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "classnames": "^2.2",
    "inline-style-prefixer": "^1.0.4",
    "invariant": "^2.2.1",
    "lodash": "^4.13.1",
    "object-assign": "^4.1.0",
    "react-avatar": "^1.2.1",
    "react-icon-base": "^2.0.0",
    "react-motion": "^0.4.4",
    "recompose": "^0.20.0",
    "warning": "^3.0.0"
  },
  "jest": {
    "automock": false,
    "testPathDirs": [
      "src"
    ],
    "unmockedModulePathPatterns": [
      "node_modules",
      "babel"
    ]
  }
}
